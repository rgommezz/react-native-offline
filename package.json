{
  "name": "react-native-offline",
  "version": "5.0.0",
  "description": "Handy toolbelt to deal with offline mode in React Native applications. Cross-platform, provides a smooth redux integration.",
  "main": "./src/index.js",
  "author": "Raul Gomez Acu√±a <raulgdeveloper@gmail.com> (https://github.com/rgommezz)",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src",
    "test": "jest test",
    "ts": "tsc --noEmit",
    "format": "eslint --fix js/**/",
    "prettier": "prettier --single-quote --trailing-comma all --write \"src/**/*.js\"",
    "test:coverage": "node --harmony node_modules/.bin/jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "precommit": "npm run lint",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rgommezz/react-native-offline.git"
  },
  "keywords": [
    "react-native",
    "react",
    "offline",
    "offline-first",
    "redux"
  ],
  "files": [
    "src"
  ],
  "bugs": {
    "url": "https://github.com/rgommezz/react-native-offline/issues"
  },
  "homepage": "https://github.com/rgommezz/react-native-offline#readme",
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.138",
    "@types/react": "^16.9.2",
    "@types/react-native": "^0.60.11",
    "@types/react-test-renderer": "^16.9.0",
    "all-contributors-cli": "^5.4.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-preset-react-native": "^4.0.1",
    "coveralls": "^3.0.6",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.11.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "jest-enzyme": "^7.0.1",
    "jsdom": "^13.1.0",
    "prettier": "^1.15.3",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-native": "^0.60.5",
    "react-native-testing-library": "^1.5.0",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "^16.6.3",
    "redux": "^4.0.1",
    "redux-mock-store": "^1.5.3",
    "redux-saga-test-plan": "4.0.0-beta.2",
    "redux-thunk": "^2.3.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.6.2"
  },
  "dependencies": {
    "@react-native-community/netinfo": "^4.1.2",
    "lodash": "^4.17.11",
    "react-redux": "^6.0.0 || ^7.0.0",
    "redux": "4.x",
    "redux-saga": "^1.0.2"
  },
  "jest": {
    "collectCoverage": true,
    "preset": "ts-jest",
    "setupTestFrameworkScriptFile": "<rootDir>/test/setupTestEnv.js",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/"
    ]
  }
}
